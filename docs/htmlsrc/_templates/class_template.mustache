<!DOCTYPE html>
<html>
<head>
    <title>Class | {{class_title}}</title>
    {{> styles }}
</head>
<body>
    <!-- main nav -->
    {{> main-nav }}

    
    <div class="row content-row" data-equalizer="content">

        

      <!-- side nav -->
      <div id="side-nav" class="columns large-2" data-equalizer-watch="content">
        <h3>Namespaces:</h3>
        {{{ namespace_nav }}}
      </div>


      <!-- main content area -->
      <div class="columns large-10" data-equalizer-watch="content">

        <div id="container">

          <div class="content">

            <!-- content -->
            <h1 id="page-title">{{class_name}} Class Reference</h1>
                        
            <!-- description and side bar -->
            <div id="description" class="row">
              <div id="description-prose" class="prose columns large-9">
                {{{description}}}
                {{{prefix}}}
              </div>

              <div class="side columns large-3">
                {{#side_nav_content}}
                    {{> side-nav }}
                {{/side_nav_content}}
              </div>
            </div>

            <hr/>

            <!-- show/hide buttons -->
            <div id="show-hide">
              <a class="show-all">Show All</a> | <a class="hide-all">Hide All</a>
            </div>
            
            <!-- contents -->
            <div class="contents">

                <!-- enumerations -->
                {{#enumerations.length}}
                <a name="{{enumerations.anchor}}"></a>
                <section>
                    <h1>Public Types</h1>
                    <ul>
                        {{#enumerations.list}}
                        <li class="row expandable">
                            <a name="{{anchor}}"></a>
                            <div class="enumDef">
                                <div class="definitionCol columns large-12">
                                    <div class="definition"><em>{{name}}</em></div>
                                    <div class="description content">
                                        <ul>
                                            {{#values}}
                                            <li>{{name}}</li>
                                            {{/values}}
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </li>
                        {{/enumerations.list}}
                    </ul>
                </section>
                <hr/>
                {{/enumerations.length}}

                <!-- public member functions -->
                {{#public_functions.length}}
                <a name="{{public_functions.anchor}}"></a>
                <section>
                    <h1>Public Member Functions</h1>
                    {{! function -> "Public Member Functions", }}
                    {{! interface -> "interface": "Instance Methods" }}
                    <ul>
                        {{#public_functions.list}}
                            {{> function-markup }}
                        {{/public_functions.list}}
                    </ul>
                </section>
                <hr/>
                {{/public_functions.length}}

                <!-- public member functions -->
                {{#public_static_functions.length}}
                <a name="{{public_static_functions.anchor}}"></a>
                <section>
                    <h1>Public Static Member Functions</h1>
                    <ul>
                        {{#public_static_functions.list}}
                            {{> function-markup }}
                        {{/public_static_functions.list}}
                    </ul>
                </section>
                <hr/>
                {{/public_static_functions.length}}
            </div>

            <!-- end content -->

          </div>
        </div>

      </div>
    </div> <!-- row -->


    {{> footer }}

    {{> scripts }}
</body>
</html>
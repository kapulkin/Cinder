                {{/list}}
            </ul>
        </div>
        {{/subnav}}
        {{/subnav.length}}

      <div id="template-content" class="columns large-12">
      
      </div>
    </div>
        
     

    {{> footer }}

    
    <script type="text/javascript">
      setSection("guides");
    </script>

</body>
</html>